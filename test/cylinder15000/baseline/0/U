FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
#include "flow.H"
dimensions      [0 1 -1 0 0 0 0];

internalField   uniform ($u 0 0);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform ($u 0 0);  
    }
    outlet
    {
        type            pressureInletOutletVelocity;
        value           uniform (0 0 0);
    }
    down
    {
        type             symmetry;
    }
    up
    {
        type             symmetry;
    }
    defaultFaces
    {
        type            symmetry;
    }
    cylinder
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
}


