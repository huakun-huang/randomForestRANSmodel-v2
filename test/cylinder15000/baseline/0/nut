FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0";
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0.000267;

boundaryField
{
    inlet
    {
        type            calculated;
        value           uniform 0.000267;
    }
    outlet
    {
        type            calculated;
        value           uniform 0.000267; 
    }
    down
    {
        type            symmetry;
    }
    up
    {
        type            symmetry;
    }
    defaultFaces
    {
        type            symmetry;
    }
    cylinder
    {
        type            nutLowReWallFunction;
        value           uniform 0.000267;
    }
}


