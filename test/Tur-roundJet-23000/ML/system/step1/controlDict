#include "paras2.H"

application             buoyantFoam;

startFrom               latestTime;

startTime               0;

stopAt                  endTime;

endTime                 10000;

deltaT                  1;

writeControl            timeStep;

writeInterval           200;

purgeWrite              5;

timeFormat              general;

timePrecision           6;

writeFormat             ascii;

writePrecision          6;

writeCompression        off;

runTimeModifiable        yes;

maxCo        1;

maxAlphaCo        0;

#include  "libDict"
functions
{
  #includeFunc     residuals
  #includeFunc     gradT
  #includeFunc     lineCenter
  //#includeFunc     lineNu
  //#includeFunc     outlet
  #includeFunc     linep
  #includeFunc     lineT
}
