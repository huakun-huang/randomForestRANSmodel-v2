solvers
{
    k
     {
		relTol	0.1;
		preconditioner	DILU;
		tolerance	1.0E-6;
		maxIter	100;
		solver	PBiCGStab;
     }
    omega
     {
		relTol	0.1;
		preconditioner	DILU;
		tolerance	1.0E-6;
		maxIter	100;
		solver	PBiCGStab;
     }
    U
     {
		relTol	0.1;
		preconditioner	DILU;
		tolerance	1.0E-6;
		maxIter	100;
		solver	PBiCGStab;
     }
    p_rgh
     {
		smoother	GaussSeidel;
		relTol	0.1;
		cacheAgglomeration	true;
		nFinestSweeps	2;
		maxIter	100;
		nPreSweeps	0;
		nPostSweeps	1;
		agglomerator	faceAreaPair;
		nCellsInCoarsestLevel	10;
		tolerance	1.0E-6;
		mergeLevels	1;
		solver	GAMG;
     }
    phi
     {
		solver	GAMG;
		smoother	GaussSeidel;
		nPreSweeps	0;
		nPostSweeps	1;
		nFinestSweeps	1;
		nCellsInCoarsestLevel	10;
		mergeLevels	2;
		agglomerator	faceAreaPair;
		cacheAgglomeration	true;
		tolerance	1.0E-8;
		relTol	0.01;
		maxIter	100;
     }
     h
     {
		relTol	0.1;
		preconditioner	DILU;
		tolerance	1.0E-6;
		maxIter	100;
		solver	PBiCGStab;
     }
}

#include "solverSettings"
#include "paras2.H"

relaxationFactors
{
    equations
    {
         U        0.2;//$relaVelocity;
         T        0.5;//$relaH;
         k        0.5;//$relaK;
         omega        0.5;//$relaOmega;
         h        0.5;//$relaH;
    }
    fields
    {
         p       0.3;//$relaPressure;
         p_rgh         0.3;//$relaPressure;
    }
}


